{
  "name": "parrotizer",
  "short_name": "parrotizer",
  "description": "Start to party with parrots",
  "version": "0.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["scripts/script.js"]
	}],  
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Start to party"
  },
  "options_page": "pages/options.html",
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": true
  }
}
